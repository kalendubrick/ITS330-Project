
public class Client {
	private int ID;
	private int port;
	private String name;
	
	public int getID() 
	{ 
		return this.ID;
	}
	public void setID(int id)
	{
		this.ID = id;
	}
	public int getPort()
	{
		return port;
	}
	public void setPort(int port)
	{
		this.port = port;
	}
	public String getName()
	{
		return name;
	}
	public void setName(String name)
	{
		this.name = name;
	}
}
