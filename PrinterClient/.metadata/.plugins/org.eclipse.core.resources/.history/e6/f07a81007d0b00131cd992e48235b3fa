
public class PrintClient extends Client
{
	private int threadID;
	
	public PrintClient(int i, int j)
	{
		this.threadID = i;
		super.setID(j);
	}
	
	public int getID()
	{
		return this.threadID;
	}
	public int getParentID()
	{
		return super.getID();
	}
	public int getThreadID()
	{
		return threadID;
	}
	public void setThreadID(int id)
	{
		this.threadID = id;
	}
}
